package edu.iut.app;

import java.util.*;

public class Agenda extends LinkedList<ExamEvent>/* EX2: COLLECTION d'ExamEvent */ {

	public Agenda() {	
	}
	
	public Agenda(LinkedList<ExamEvent> exams){
		super(exams);
	}
	
	public void addCheckedEvent(ExamEvent examEvent) {
		this.add(examEvent);
	}
	
}
